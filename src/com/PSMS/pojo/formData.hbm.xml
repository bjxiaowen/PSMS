<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.PSMS.pojo.FromData" table="InverterDataInfo">
		<id name="InverterDataID" type="java.lang.Integer">
			<column name="InverterDataID" />
			<generator class="assigned"/>
		</id>
		
		<property name="InverterID" type="java.lang.String">
			<column name="InverterID" />
		</property>
		
		<property name="ReceiveData" type="java.lang.String">
			<column name="ReceiveData" />	
		</property>
		
		<property name="OperateDate" type="java.util.Date">
			<column name="Date" />	
		</property>
		
		<property name="Type" type="java.lang.String">
			<column name="Type"/>
		</property>
	</class>
</hibernate-mapping>
